<div class="container">
<h2 class="header">List of all subjects</h2><br/>
	<table class="table">
		<tr>
			<th>subject_id</th>
			<th>subject_name</th>
			<th>class_name</th>
		</tr>
		<c:forEach var="Subject" items="${requestScope.subjects}">
			<tr>
				<td>${Subject.subject_id}</td>
				<td>${Subject.subject_name}</td>
				<td>${Subject.class_name}</td>
			</tr>
		</c:forEach>
	</table>
